path /users/summary/{uid} is UserSummary;

type UserSummary {
  read() { !isBlocked(uid) }
  age: Number | Null,
  avatar: String | Null,
  display_name: String | Null,
  gender: String | Null,
  network: String | Null,
  is_online: IsOnline | Null,
}

type IsOnline extends Boolean {
  write() { isCurrentUser(uid) }
}
